        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^25.4.0",
        "bootstrap": "^4.4.1",
        "cross-env": "^7.0",
        "expect": "^25.4.0",
        "jest": "^25.4.0",
        "jquery": "^3.5.0",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^5.0.4",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
